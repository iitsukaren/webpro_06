<!Doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>高層建築物</title>
  <link rel="stylesheet" href="./../public/style/buildstyle.css">
</head>
<body>
  <h1>世界の高層建築物</h1>
  <hr>
  <table>
    <thead>
        <tr>
            <th>ID</th>
            <th>名称</th>
            <th>分類</th>
        </tr>
    </thead>
    <tbody>
        <% let number = 0 %>
            <% for( let row of data ) { %>
                <tr>
                    <td><%= row.id %></td>
                    <td><a href="/<%= number %>"><%= row.name %></a></td>
                    <td><%= row.type %></td>
                </tr>
            <% number += 1 %>
        <% } %>
    </tbody>
  </table>
  <a  href="/create" class="linkplace">高層建築物の追加</a>
</body>
</html>